<%- include('./partials/head') %>

<div class="container-fluid">
  <div class="row">
      <div class="col-sm-2">
        <%# Sidebar menu %>
        <%- include('./partials/sidebar') %>

      </div>

      <div class="col-sm-10">
        <%# Body content %>
        <h1><%= title %></h1>
          <% if (genre_list.length) { %>
            <ul>
              <% genre_list.forEach(genre => { %>
                <li>
                  <a href='genre.url'> <%= genre.name %> </a>
                </li>
              <% }) %>
            </ul>
            <% } else { %>
             <p>There are no genres to list.</p>
          <% } %>
      </div>

  </div>
</div>

<%- include('./partials/footer') %>